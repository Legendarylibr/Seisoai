# FAL.ai API Configuration
# Get your API key from https://fal.ai
VITE_FAL_API_KEY=your_fal_api_key_here

# FastAPI/ComfyUI Configuration (for free NFT users)
VITE_FASTAPI_URL=http://localhost:8000
VITE_FASTAPI_ENABLED=true

# API Configuration
VITE_API_URL=http://localhost:3001

# CDN Configuration (for production)
VITE_CDN_URL=https://your-cdn-domain.com

# Payment Wallet Addresses (REQUIRED)
VITE_ETH_PAYMENT_WALLET=0x1234567890123456789012345678901234567890
VITE_POLYGON_PAYMENT_WALLET=0x1234567890123456789012345678901234567890
VITE_ARBITRUM_PAYMENT_WALLET=0x1234567890123456789012345678901234567890
VITE_OPTIMISM_PAYMENT_WALLET=0x1234567890123456789012345678901234567890
VITE_BASE_PAYMENT_WALLET=0x1234567890123456789012345678901234567890
VITE_SOLANA_PAYMENT_WALLET=So11111111111111111111111111111111111111112

# RPC Endpoints (REQUIRED for crypto payments - used for balance checking and network detection)
VITE_ETH_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_API_KEY
VITE_POLYGON_RPC_URL=https://polygon-mainnet.g.alchemy.com/v2/YOUR_API_KEY
VITE_ARBITRUM_RPC_URL=https://arb-mainnet.g.alchemy.com/v2/YOUR_API_KEY
VITE_OPTIMISM_RPC_URL=https://opt-mainnet.g.alchemy.com/v2/YOUR_API_KEY
VITE_BASE_RPC_URL=https://base-mainnet.g.alchemy.com/v2/YOUR_API_KEY
VITE_SOLANA_RPC_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_HELIUS_API_KEY

# Monitoring Configuration
VITE_SENTRY_DSN=your_sentry_dsn_here

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Stripe Configuration (REQUIRED for card payments) - LIVE MODE
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_publishable_key_here