{
  "name": "seiso-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "sideEffects": [
    "*.css",
    "src/index.css",
    "src/main.tsx"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "start": "node --import tsx start-app.ts",
    "start:backend": "cd backend && node --import tsx server-modular.ts",
    "build:start": "npm run build && npm start",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "docker:build": "docker build -t seiso-ai:latest .",
    "docker:build:frontend": "docker build -f Dockerfile.frontend -t seiso-frontend:latest .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "clean": "rm -rf dist node_modules/.vite"
  },
  "dependencies": {
    "@fal-ai/client": "^1.6.2",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@google/model-viewer": "^4.1.0",
    "@solana/spl-token": "^0.1.8",
    "@solana/web3.js": "^1.98.4",
    "@stripe/react-stripe-js": "^5.3.0",
    "@stripe/stripe-js": "^8.1.0",
    "@walletconnect/ethereum-provider": "^2.23.1",
    "axios": "^1.12.2",
    "buffer": "^6.0.3",
    "ethers": "^6.7.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsx": "^4.7.1"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
