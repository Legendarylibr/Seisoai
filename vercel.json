{
  "version": 2,
  "name": "seiso-ai",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "@vite_api_url",
    "VITE_FAL_API_KEY": "@vite_fal_api_key",
    "VITE_STRIPE_PUBLISHABLE_KEY": "@vite_stripe_publishable_key"
  }
}
