{
  "name": "ai-image-generator-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Backend API for AI Image Generator with wallet-based user management",
  "main": "server-modular.ts",
  "scripts": {
    "start": "tsx server-modular.ts",
    "dev": "nodemon --exec tsx server-modular.ts",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest --passWithNoTests",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage",
    "lint": "eslint . --ext .ts --ignore-pattern 'node_modules/' --ignore-pattern 'dist/'",
    "lint:fix": "eslint . --ext .ts --fix --ignore-pattern 'node_modules/' --ignore-pattern 'dist/'",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "clean": "rm -rf dist coverage"
  },
  "keywords": [
    "ai",
    "image-generation",
    "blockchain",
    "wallet",
    "nft",
    "crypto"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@solana/web3.js": "^1.98.4",
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.34.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.7.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "opossum": "^8.1.4",
    "stripe": "^19.1.0",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^10.0.0",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.0",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "jest": "^29.6.2",
    "@jest/globals": "^29.6.2",
    "ts-jest": "^29.1.0",
    "nodemon": "^3.0.1",
    "supertest": "^7.0.0",
    "@types/supertest": "^6.0.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
